{
  "name": "@eeko/cli",
  "version": "0.3.0",
  "description": "CLI for local Eeko widget development",
  "author": "Eeko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EekoBot/cli.git"
  },
  "bugs": {
    "url": "https://github.com/EekoBot/cli/issues"
  },
  "homepage": "https://github.com/EekoBot/cli#readme",
  "type": "module",
  "bin": {
    "eeko": "./bin/eeko.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "start": "node bin/eeko.js",
    "prepublishOnly": "pnpm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@eeko/sdk": "^0.1.5",
    "@supabase/supabase-js": "^2.86.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "get-port": "^7.1.0",
    "giget": "^2.0.0",
    "ink": "^5.1.0",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "open": "^10.1.0",
    "react": "^18.3.1",
    "vite": "^6.0.0",
    "ws": "^8.18.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/ws": "^8.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "eeko",
    "cli",
    "widget",
    "streaming",
    "overlay",
    "development",
    "twitch",
    "youtube",
    "kick",
    "tiktok"
  ]
}
